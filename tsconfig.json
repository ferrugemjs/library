{
    "compilerOptions": {
        "target": "es5"
        ,"module": "amd"
        ,"moduleResolution": "classic"
        ,"outDir": "./dist"
        ,"experimentalDecorators": true
        ,"noImplicitAny":true		
        ,"sourceMap": false        
		,"declaration": true
		,"removeComments":true
        ,"noEmitHelpers":true
        ,"importHelpers":true 
        ,"suppressImplicitAnyIndexErrors": true
        ,"baseUrl": "./"
        ,"lib":["es2015", "dom","es5","scripthost","es6"]
        ,"paths": {
            "tslib" : ["node_modules/tslib/tslib.d.ts"]
        }
    },
    "exclude": [
        "node_modules"
		,"dist"
    ],
    "compileOnSave": false
}
