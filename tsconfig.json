{
    "compilerOptions": {
        "target": "es5"
        ,"module": "amd"
        ,"moduleResolution": "classic"
        ,"outDir": "./dist"
        ,"experimentalDecorators": true
        ,"noImplicitAny":true		
        ,"sourceMap": false        
		,"declaration": false
		,"removeComments":true
		,"noEmitHelpers":true 
		,"suppressImplicitAnyIndexErrors": true
        ,"lib":["dom","es6"]
    },
    "exclude": [
        "node_modules"
		,"dist"
    ],
    "compileOnSave": false
}
